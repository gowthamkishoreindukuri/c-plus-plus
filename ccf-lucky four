#include <iostream>
using namespace std;
int main()
{
    int t, n, x, i;
    int count;
    cin >> t;

    for (i = 1; i <= t; i++)
    {
        cin >> n;
        count = 0;
        while (n != 0)
        {
            x = n % 10;
            if (x == 4)
            {
                count ++;
            }
            n = n / 10;
        }
        cout << count << endl;
    }

    return 0;
}
